# Hello world
print("Hello world\t  Hello world")

# Hello world with tab
print("Hello world\n  Hello world")

# Hello World With new line
print("Hello world\t  hello world")

# Integer Input
a=int(input())
print(a)

# Float Input
a=float(input())
print(a)

# Character Input
a=input()
print(a)

# String Input
a=str(input())
print(a)

# Ascii-I
a=input()
print(ord(a))

# Ascii-II
a=int(input())
print(chr(a))
